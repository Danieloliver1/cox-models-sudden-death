]

# Fun√ß√£o para calcular estat√≠sticas
def calcular_estatisticas(ondas):
    if ondas.empty:  
        return {stat: 0 for stat in ["mean", "std", "max", "min", "25%", "50%", "75%"]}
    
    # Substituir valores NaN por zero antes de calcular as estat√≠sticas
    ondas = ondas.fillna(0)

    # Evitar divis√£o por zero substituindo valores muito pequenos
    ondas[ondas == 0] = 1e-6
    
    estatisticas = {
        "mean": ondas.mean(),
        "std": ondas.std(),
        "max": ondas.max(),
        "min": ondas.min(),
        "25%": ondas.quantile(0.25),
        "50%": ondas.quantile(0.50),
        "75%": ondas.quantile(0.75)
    }
    
    return estatisticas

